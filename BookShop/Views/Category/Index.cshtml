@model IEnumerable<Category>
    
    <h1> Index category</h1>

    <table class="table table-bordered table-striped">
        
        <thead>
        
        <th>Category name</th>
        <th>Display Order</th>
        </thead>
        
        <body>
        
        @foreach ( var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.DisplayOrder</td>
            </tr>
        }
        </body>
    </table>